{
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx"
        ]
      }
    ]
    //should add ".ts" if typescript project
  },
  "extends": [
    "plugin:react/jsx-runtime"
  ]
}